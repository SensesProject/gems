{
  "title": "Scenario Sensitivities",
  "description": "This GEM show the difference between scenarios that pursue different policies on decisive side indicators.",
  "default": {
    "runs": [
      ["REMIND-MAgPIE 1.7-3.0", "PEP_${warming}_${cdr}_NDC"],
      ["REMIND-MAgPIE 1.7-3.0", "PEP_${warming}_${cdr}_netzero"],
      ["REMIND-MAgPIE 1.7-3.0", "PEP_${warming}_${cdr}_eff"]
    ] ,
    "regions": ["World"],
    "primaryDimension": "scenarios"
  },
  "dropdowns": [{
    "label": "Perspective",
    "options": [ {
      "label": "How do different policies affect climate diagnostics?",
      "variables": [
        "Emissions|Kyoto Gases", 
        "AR5 climate diagnostics|Temperature|Exceedance Probability|1.5 °C|FAIR",
        "AR5 climate diagnostics|Temperature|Exceedance Probability|2.0 °C|FAIR",
        "AR5 climate diagnostics|Temperature|Global Mean|FAIR|MED"
      ],
      "regions": ["World"]
      
    },{
      "label": "Which are large scale economic costs?",
      "variables": [
        "Price|Carbon",
        "Policy Cost|GDP Loss" 
      ],
      "regions": ["World"] 
    },{
      "label": "What are effects for end-users?",
      "variables": [
        "Price|Agriculture|Corn|Index",
        "Price|Agriculture|Soybean|Index",
        "Price|Agriculture|Wheat|Index",
        "Price|Agriculture|Livestock|Index", 
        "Price|Final Energy|Residential|Electricity", 
        "Price|Final Energy|Residential|Gases|Natural Gas", 
        "Price|Final Energy|Residential|Liquids|Biomass", 
        "Price|Final Energy|Residential|Liquids|Oil", 
        "Price|Final Energy|Residential|Solids|Coal", 
        "Price|Final Energy|Residential|Solids|Biomass"
      ],
      "regions": ["World"],
      "groups": [{
        "label": "Food price index",
        "size": 4
      }, {
        "label": "Final energy prices for the residential sector",
        "size": 3
      }, {
        "label": " ",
        "size": 3
      }] 
    }]
  }, {
    "label": "Warming Level",
    "options": [{
      "label": "2°C",
      "warming": "2C"
    }, {
      "label": "1.5°C",
      "warming": "1p5C"
