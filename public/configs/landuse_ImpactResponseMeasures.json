    "runs":[
      ["REMIND-MAgPIE 1.7-3.0", "SMP_${warming}_Def"],
      ["REMIND-MAgPIE 1.7-3.0", "SMP_${warming}_Sust"],
      ["REMIND-MAgPIE 1.7-3.0", "SMP_${warming}_early"],
      ["REMIND-MAgPIE 1.7-3.0", "SMP_${warming}_lifesty"],
      ["REMIND-MAgPIE 1.7-3.0", "SMP_${warming}_regul"]
