      "variables": [
        "Land Cover|Forest|Afforestation and Reforestation",
        "Land Cover|Cropland",
        "Land Cover|Cropland|Energy Crops",
        "Land Cover|Pasture",
        "Water Consumption|Irrigation", 
        "Fertilizer Use|Nitrogen", 
        "Agricultural Production", 
        "Agricultural Production|Energy|Crops", 
        "Agricultural Production|Non-Energy|Crops",
        "Agricultural Production|Non-Energy|Livestock"
      ],
      "groups": [{
        "label": "Change in land cover",
        "size": 4
      }, {
        "label": "Change in irrigation and fertilizer use",
        "size": 2
      },{
        "label": "Change in yield",
        "size": 4
      }]
    }, {
      "label": "What is the influence on food security?",
      "variables": [
        "Population", 
        "Food Demand",
        "Price|Agriculture|Non-Energy Crops and Livestock|Index",
        "Land Cover|Forest|Afforestation and Reforestation",
        "Land Cover|Cropland|Energy Crops",
        "Land Cover|Cropland|Cereals",
        "Land Cover|Pasture"
      ], 
      "groups": [{
        "label": "Price index",
        "size": 3
